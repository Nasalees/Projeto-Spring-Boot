package com.projetoSpring.userdept.entities;

public class User {
	private long id;
	private String name;
	private String email;
	
	public User() {
		
	}
	
	public void setID(long id) {
		this.id = id;
	}
	public long getId() {
		return this.id;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return this.name;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getEmail() {
		return this.email;
	}
}
